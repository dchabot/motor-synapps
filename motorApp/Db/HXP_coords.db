record(bo,"$(P)$(R)READ_CS") {
    field(DESC,"Read Coord Systems")
    field(DTYP,"asynInt32")
    field(ZNAM,"Done")
    field(ONAM,"Read")
    field(OUT, "@asyn($(PORT),0)HXP_COORD_SYS_READ_ALL")
    field(VAL, "1")
    field(PINI,"YES")
}

